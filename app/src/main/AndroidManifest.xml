<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.healthcareapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:background="@color/colorPrimary"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthCareApp">
        <activity android:name=".Controllers.StartActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:background="@color/colorPrimary">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name=".Controllers.RegisterActivity"
            android:label="Register">
        </activity>
        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name=".Controllers.LoginActivity"
            android:label="Login"></activity>
        <activity
            android:name=".Controllers.MainActivity"
            android:label="Care For U"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>